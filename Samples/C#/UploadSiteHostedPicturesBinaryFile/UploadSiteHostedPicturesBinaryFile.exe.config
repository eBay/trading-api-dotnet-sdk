<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <appSettings>
    <!-- API Server URL, required -->
    <!-- For production site use: https://api.ebay.com/wsapi -->
    <!-- For Sandbox site use: https://api.sandbox.ebay.com/wsapi -->
    <add key="Environment.ApiServerUrl" value="https://api.ebay.com/ws/api.dll"/>
    <add key="Environment.EPSServerURL" value="https://api.ebay.com/ws/api.dll"/>
    <!-- User token for API server access, required -->
    <add key="UserAccount.ApiToken" value=""/>
  </appSettings>
</configuration>
